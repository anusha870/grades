#incude<stdio.h>
int main()
{
float m1,m2,m3,m4,m5,aver;
printf("enter the 5 subject marks");
scanf("%f %f %f %f %f",&m1,&m2,&m3,&m4,&m5);
aver=(m1+m2+m3+m4+m5)/5;
if(aver>=85 && aver<=100)
{
printf("grade A\n");
}
else if(aver>=70 && aver<=84)
{
printf("grade B\n");
}
else if(aver>=55 && aver<=69)
{
printf("grade C\n");
}
else if(aver>=40 && aver<=54)
{
printf("grade D\n");
}
else 
{
printf("grade F\n");
}
return 0;
}
